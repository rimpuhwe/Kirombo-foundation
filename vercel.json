{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
   "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "abdallahkirombafoundation.com"
        }
      ],
      "destination": "https://www.abdallahkirombafoundation.org/$1",
      "permanent": true
    }
  ]
}
